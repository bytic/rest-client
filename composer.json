{
  "name": "bytic/rest-client",
  "license": "MIT",
  "description": "bytic rest client",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\RestClient\\": "src/"
    }
